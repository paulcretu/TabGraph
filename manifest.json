{
  "manifest_version": 2,
  "name": "TabGraph",
  "version": "1.0.1",
  "description": "Track and graph how many tabs you have open.",
  "browser_action": {
    "default_icon": "images/icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}