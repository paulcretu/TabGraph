{
  "manifest_version": 2,
  "name": "TabGraph",
  "version": "1.0.0.0",
  "description": "A tab graphing utility for Chrome.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs"
  ]
}